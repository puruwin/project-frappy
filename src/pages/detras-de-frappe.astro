---
import Layout from "../layouts/Layout.astro";
import "../styles/index.css";
import BrutalHeader from "../components/BrutalHeader.astro";
import * as content from "../content/detras-de-frappe.md";

console.log(content);
---

<Layout title="Detrás de Frappé">
  <BrutalHeader navItems={[]} isPost={true} />
  <main
    class="container mx-auto mt-[60px] min-h-[calc(100vh-60px)] max-w-5xl px-4 pt-16 sm:mt-[76px] sm:min-h-[calc(100vh-76px)] sm:px-8"
  >
    <div class="prose max-w-none lg:prose-xl">
      <content.Content />
    </div>
  </main>
</Layout>
